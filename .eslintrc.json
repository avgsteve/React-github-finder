{
  "env": {
    "browser": true,
    "es2020": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "standard"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [2, "always"],
    "space-in-parens": ["error", "always"],
    "computed-property-spacing": ["error", "always"]
  }
}